#!/usr/bin/env ruby
require_relative "../environment"

require_relative "../lib/point"

part1 answer: 2344 do
  Grid.parse(input).count("XMAS")
end

part2 answer: 1815 do
  Grid.parse(input).count_x("MAS")
end

class Grid
  def self.parse(input)
    points = {}
    input.lines(chomp: true).each.with_index do |line, y|
      line.chars.each.with_index do |char, x|
        points[Point[x, y]] = char
      end
    end
    new(points)
  end

  def initialize(points) = @points = points

  def count(target)
    @points.sum do |p, char|
      Point.origin.principals.sum do |dir|
        next_p = p
        extent = char
        (target.size - 1).times do
          next_p += dir
          extent += @points[next_p].to_s
        end
        extent == target ? 1 : 0
      end
    end
  end

  def count_x(target)
    @points.each_key.count do |p|
      first_diag = to_extent(p + Point[-1, -1], p, p + Point[1, 1])
      secnd_diag = to_extent(p + Point[-1, 1], p, p + Point[1, -1])

      is_reversible?(first_diag, target) && is_reversible?(secnd_diag, target)
    end
  end

  private

  def is_reversible?(str, target)
    str == target || str.reverse == target
  end

  def to_extent(*points) = points.map { @points[it].to_s }.join
end

__END__
